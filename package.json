{
  "name": "nextjs-boilerplate",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "format": "prettier --ignore-path .gitignore \"**/*.+(ts|js|tsx)\" --write",
    "postinstall": "node ./bin/postInstall",
    "start": "next start",
    "patch": "patch-package",
    "test": "jest",
    "lint": "tsc --project  ./tsconfig.json --noEmit && eslint  --ext=jsx,ts,tsx,js,css,scss && next lint",
    "generate:release": "standard-version",
    "cypress": "cypress open",
    "postbuild": "next-sitemap",
    "storybook": "start-storybook -p 6006 -s ./public/locales",
    "build-storybook": "build-storybook -s public",
    "create-component": "node create-component.js"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@reduxjs/toolkit": "^1.9.3",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "axios": "^1.3.4",
    "i18next": "^22.4.13",
    "inquirer": "^8.0.0",
    "next": "13.2.4",
    "next-i18next": "^13.2.2",
    "next-seo": "^5.15.0",
    "next-sitemap": "^4.0.6",
    "react": "18.2.0",
    "react-dark-mode-toggle": "^0.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.2.0",
    "react-redux": "^8.0.5",
    "typescript": "5.0.2",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@commitlint/cli": "17.5.0",
    "@commitlint/config-conventional": "17.4.4",
    "@next/eslint-plugin-next": "^13.2.4",
    "@percy/cypress": "^3.1.2",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@types/jest": "^29.5.0",
    "@types/node": "18.15.5",
    "@types/react": "18.0.28",
    "@types/react-csv": "^1.1.3",
    "@types/react-dom": "18.0.11",
    "@types/react-spinkit": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "babel-loader": "^8.3.0",
    "cypress": "^12.9.0",
    "eslint": "8.36.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-cypress": "^2.13.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-testing-library": "^5.10.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.0",
    "patch-package": "^6.5.1",
    "prettier": "^2.8.6",
    "standard-version": "^9.5.0",
    "storybook-addon-next-router": "^4.0.2"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint",
      "yarn format"
    ]
  }
}
